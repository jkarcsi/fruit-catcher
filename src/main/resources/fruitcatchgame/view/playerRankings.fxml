<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.Button?>
<?import javafx.scene.control.TableColumn?>
<?import javafx.scene.control.TableView?>
<?import javafx.scene.layout.VBox?>

<VBox xmlns="http://javafx.com/javafx" xmlns:fx="http://javafx.com/fxml" fx:controller="controller.PlayerRankingsController" spacing="10" alignment="CENTER">
    <TableView fx:id="rankingsTable" prefWidth="400">
        <columns>
            <TableColumn fx:id="usernameColumn" text="Username" prefWidth="200"/>
            <TableColumn fx:id="scoreColumn" text="Score" prefWidth="200"/>
        </columns>
    </TableView>
    <Button text="Back to Main Menu" onAction="#handleBackToMainMenuButton"/>
</VBox>
